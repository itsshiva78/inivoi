{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/invoice/generate/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/invoice/send/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/invoice/export/route.ts": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}